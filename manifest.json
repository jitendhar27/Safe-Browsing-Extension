{
    "manifest_version": 3,
    "name": "Safe Browsing Extension",
    "version": "1.0",
    "description": "Warns users about malicious websites using public threat databases.",
    "permissions": ["webRequest", "activeTab", "storage"],
    "host_permissions": ["*://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon-shield.png",
        "48": "icons/icon-shield.png",
        "128": "icons/icon-shield.png"
      }
    },
    "icons": {
        "16": "icons/icon-shield.png",
        "48": "icons/icon-shield.png",
        "128": "icons/icon-shield.png"
    }

  }
  